#!/usr/bin/python3
import sys


if __name__ == '__main__':
    if len(sys.argv) == 2:
        try:
            file1 = open(sys.argv[1], "r")
        except Exception:
            exit()
        for line in file1:
            try:
                num = int(line[:-1])
                print('{}=1*{}'.format(num, num))
            except Exception:
                continue
        file1.close()
